@import '../../styles'

.notFound
    width 60%
    margin 20px 20% 40px 20%
    float left 
    &__box
        width calc(100% - 120px)
        padding 40px 60px 40px 60px
        float left
    &--error
        color warningColor
    &--content
        font-size 15px
    &--styles
        border 3px solid warningColor
        border-radius(10px)
        text-align center